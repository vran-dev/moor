{
  "name": "moor",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.8.1",
    "@codemirror/commands": "^6.2.4",
    "@codemirror/language": "^6.8.0",
    "@codemirror/language-data": "^6.3.1",
    "@codemirror/state": "^6.2.1",
    "@codemirror/view": "^6.14.1",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@excalidraw/excalidraw": "^0.15.2-3ddcc48",
    "@floating-ui/react": "^0.25.0",
    "@lexical/code": "^0.11.3",
    "@lexical/file": "^0.11.3",
    "@lexical/link": "^0.11.3",
    "@lexical/list": "^0.11.3",
    "@lexical/mark": "^0.11.3",
    "@lexical/overflow": "^0.11.3",
    "@lexical/react": "^0.11.3",
    "@lexical/rich-text": "^0.11.3",
    "@lexical/selection": "^0.11.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-virtual": "^3.0.0-beta.54",
    "@uiw/codemirror-theme-github": "^4.21.9",
    "antd": "^5.8.2",
    "dayjs": "^1.11.9",
    "electron-updater": "^5.3.0",
    "emoji-mart": "^5.5.2",
    "lexical": "^0.11.3",
    "mermaid": "^10.2.4",
    "react-arborist": "^3.2.0",
    "react-icons": "^4.10.1",
    "react-redux": "^8.1.1",
    "throttle-debounce": "^5.0.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "electron": "^24.4.1",
    "electron-builder": "^23.4.0",
    "electron-vite": "^1.0.23",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "postcss": "^8.4.25",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
